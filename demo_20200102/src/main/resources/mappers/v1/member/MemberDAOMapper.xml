<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kail.demo.member.dao.MemberDAO">

	<select id="viewMember" parameterType="MemberVO" resultType="MemberVO">
		SELECT
			num
			,userNm
			,phone
		FROM demoDb.member
		WHERE userNm = "kafa"
    </select>
	
	<select id="test" resultType="java.lang.String">
		SELECT NOW()
	</select>

</mapper>