<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kail.member.dao.MemberDAO">

	<select id="viewMember" parameterType="MemberVO" resultType="MemberVO">
		SELECT ID
			,NAME
			,COUNTRY
			,POPULATION
		FROM CITY
		WHERE ID = #{userId}
    </select>

</mapper>